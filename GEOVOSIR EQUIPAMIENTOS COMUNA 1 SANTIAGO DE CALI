        var map = L.map('map', {
            zoomControl:true, maxZoom:18, minZoom:10
        }).fitBounds([[3.424928790323159,-76.59345903159236],[3.4797253260021703,-76.50147607730365]]);
        var hash = new L.Hash(map);
        map.attributionControl.setPrefix('<a href="https://github.com/tomchadwin/qgis2web" target="_blank">qgis2web</a> &middot; <a href="https://leafletjs.com" title="A JS library for interactive maps">Leaflet</a> &middot; <a href="https://qgis.org">QGIS</a>');
        var autolinker = new Autolinker({truncate: {length: 30, location: 'smart'}});
        var bounds_group = new L.featureGroup([]);
        function setBounds() {
        }
        function pop_Perimetro_urbano_0(feature, layer) {
            var popupContent = '<table>\
                    <tr>\
                        <td colspan="2">' + (feature.properties['nombre'] !== null ? autolinker.link(feature.properties['nombre'].toLocaleString()) : '') + '</td>\
                    </tr>\
                    <tr>\
                        <td colspan="2">' + (feature.properties['anno'] !== null ? autolinker.link(feature.properties['anno'].toLocaleString()) : '') + '</td>\
                    </tr>\
                </table>';
            layer.bindPopup(popupContent, {maxHeight: 400});
        }

        function style_Perimetro_urbano_0_0(feature) {
            switch(String(feature.properties['nombre'])) {
                case 'PERIMETRO URBANO':
                    return {
                pane: 'pane_Perimetro_urbano_0',
                opacity: 1,
                color: 'rgba(35,35,35,1.0)',
                dashArray: '',
                lineCap: 'butt',
                lineJoin: 'miter',
                weight: 1.0, 
                fill: true,
                fillOpacity: 1,
                fillColor: 'rgba(225,175,125,1.0)',
                interactive: true,
            }
                    break;
                default:
                    return {
                pane: 'pane_Perimetro_urbano_0',
                opacity: 1,
                color: 'rgba(35,35,35,1.0)',
                dashArray: '',
                lineCap: 'butt',
                lineJoin: 'miter',
                weight: 1.0, 
                fill: true,
                fillOpacity: 1,
                fillColor: 'rgba(207,118,54,1.0)',
                interactive: true,
            }
                    break;
            }
        }
        map.createPane('pane_Perimetro_urbano_0');
        map.getPane('pane_Perimetro_urbano_0').style.zIndex = 400;
        map.getPane('pane_Perimetro_urbano_0').style['mix-blend-mode'] = 'normal';
        var layer_Perimetro_urbano_0 = new L.geoJson(json_Perimetro_urbano_0, {
            attribution: '',
            interactive: true,
            dataVar: 'json_Perimetro_urbano_0',
            layerName: 'layer_Perimetro_urbano_0',
            pane: 'pane_Perimetro_urbano_0',
            onEachFeature: pop_Perimetro_urbano_0,
            style: style_Perimetro_urbano_0_0,
        });
        bounds_group.addLayer(layer_Perimetro_urbano_0);
        map.addLayer(layer_Perimetro_urbano_0);
